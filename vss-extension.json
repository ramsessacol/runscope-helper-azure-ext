{
    "manifestVersion": 1,
    "id": "runscopetesthelper",
    "publisher": "RamsesAlmeida",
    "version": "1.0.0",
    "name": "Task for Runscope integration Tests",
    "description": "Allows you to run url triggers from the runscope inside your pipeline and present the result",
    "public": false,
    "links": {
        "home": { "uri": "https://github.com/ramsessacol/runscope-helper-azure-ext#readme" },
        "repository": { "uri": "https://github.com/ramsessacol/runscope-helper-azure-ext" },
        "support": { "uri": "https://github.com/ramsessacol/runscope-helper-azure-ext/issues" },
        "getstarted": { "uri": "https://github.com/ramsessacol/runscope-helper-azure-ext#readme" }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "test",
        "tests",
        "integration",
        "smoke",
        "runscope",
        "api",
        "release",
        "build"
    ],
    "icons": {
        "default": ""
    },
    "targets": [
        { "id": "Microsoft.VisualStudio.Services" }
    ],
    "content": {
        "details": { "path": "README.md" },
        "license": { "path": "LICENSE" }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/ramsessacol/runscope-helper-azure-ext"
    },
    "contributions": [
        {
            "id": "",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": { "name": "task" }
        }
    ],
    "files": [
        { 
            "path": "task" 
        },
        { 
            "path": "images", 
            "addressable": true 
        },
        { 
            "path": "node_modules", 
            "packagePath": "task/node_modules" 
        }
    ]
}